<div id="forms">
  <!-- change this action to whatever it needs to be to send the data -->
  <form action="/home" method="post">
    <label for="classesWanted">Classes Wanted</label>
    <input {{#if classesWanted}}value="{{classesWanted}}"{{/if}} type="text" id="classesWanted" name="classesWanted" placeholder="List classes you want to take..">

    <label for="classesTaken">Classes Taken</label>
    <input {{#if classesTaken}}value="{{classesTaken}}"{{/if}} type="text" id="classesTaken" name="classesTaken" placeholder="List classes you have taken..">

    <input type="submit" value="Submit" onclick="">
  </form>
</div>
